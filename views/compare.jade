extends layout

block content
  
  nav.top-nav
    a(href="/")
        img(class="logo small" src="/images/assets/logos/carShare_text_logo_white.png")
    //- div.page-nav-links
    //-   a(href="/") Home
    //-   a(href="/list") List Page
      //- a(href="#") Page 2
    span(id="openBtn" aria-hidden="true")
      svg(version='1.1' x='0px' y='0px' width='30px' height='30px' viewBox='0 0 30 30' enable-background='new 0 0 30 30')
        rect(width='30' height='3')
        rect(y='12' width='30' height='3')
        rect(y='24' width='30' height='3')
    span(id="closeBtn" aria-hidden="true")
      svg(version="1.1" width='30px' height='30px')
        line(x1="0" x2="30" y1="0" y2="30" stroke-width="3" stroke="red" fill-opacity="0.5" stroke-opacity="0.8" stroke-linecap="square")
        line(x1="30" x2="0" y1="0" y2="30" stroke-width="3" stroke="red" fill-opacity="0.5" stroke-opacity="0.8" stroke-linecap="square")
    div.compare-now-button
      a(href="/list")
        button.primary back to list
  br
  div#slide-nav.menu-container
    div.nav-bar
        ul
          li 
            a(href="/")
              button Home
          li 
            a(href="/list")
              button List
          li 
            a(href="/compare")
              button.selected Compare
        button#add Add Another
  div(class="center-column-black")
      h1#compare-heading Compare Page
